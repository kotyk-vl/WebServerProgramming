@using Microsoft.AspNetCore.Identity;
@model IEnumerable<IdentityUser>

<h2>List of Users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Roles</a>
            </td>
        </tr>
    }
</table>